{
  "expo": {
    "name": "DocSync",
    "slug": "docsync-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7f5f3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maccari78.docsyncmobile"
    },
    "android": {
      "package": "com.maccari78.docsyncmobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f7f5f3"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.683662994531-e68duk2jnj1famoehj2ot8279q3b9gka"
        }
      ]
    ],
    "extra": {
      "googleWebClientId": "683662994531-e68duk2jnj1famoehj2ot8279q3b9gka.apps.googleusercontent.com",
      "eas": {
        "projectId": "2d564048-cc34-4d1f-93e1-584080e28f97"
      }
    }
  }
}
